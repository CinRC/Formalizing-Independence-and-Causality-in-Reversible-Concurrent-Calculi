                             [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synl FL2 LL2 HL2 FR2 LR2 HR2) (tr_s* (st_s* (sc (bwc (bc_parr BR3 HR3 NR3)))) (st_s* (sc (bwc (bc_parl BL3 HL3 NL3))))) (rch (std_par DL DR) (tr_s* (st_s* (sc (fwc (fc_parl FL3 HL3 NL3)))) (st_s* (sc (fwc (fc_parr FR3 HR3 NR3))))))])))
            | [g ⊢ v_synr VL2 VR2 LL2 HL2 LR2 HR2] ⇒
              let [g ⊢ E1] = conn_rel_two_base [g ⊢ VL1] [g ⊢ VL2] [g ⊢ C1] [g ⊢ nt_out LL1] [g ⊢ nt_out LL2] in
              let [g ⊢ E2] = conn_rel_two_base [g ⊢ VR1] [g ⊢ VR2] [g ⊢ C2] [g ⊢ nt_inp LR1] [g ⊢ nt_inp LR2] in
              (case [g ⊢ E1] of
                 | [g ⊢ c_id DL FL1 FL2] ⇒ (case [g ⊢ E2] of
                      | [g ⊢ c_id DR FR1 FR2] ⇒
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) id_s* (rch (std_par DL DR) id_s*)]
                      | [g ⊢ c_fw DR FR1 FR3 FR2 _ _] ⇒ let [g ⊢ FR3]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ FR3] in
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ DL] in
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (fwc (fc_parr FR3 H3 N3)))) (rch (std_par DL DR) id_s*)]
                      | [g ⊢ c_bw DR FR1 BR3 FR2 _ _] ⇒ let [g ⊢ FR3]:[g ⊢ fstep_cl _ T3 _] = loop_lemma_two_cl [g ⊢ BR3] in
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ DL] in
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (bwc (bc_parr BR3 H3 N3)))) (rch (std_par DL DR) (st_s* (sc (fwc (fc_parr FR3 H3 N3)))))])
                 | [g ⊢ c_fw DL FL1 FL3 FL2 LL1' LL3] ⇒ let [g ⊢ FL3]:[g ⊢ fstep_cl _ TL3 _] = [g ⊢ FL3] in
                   (case [g ⊢ E2] of
                      | [g ⊢ c_id DR FR1 FR2] ⇒
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ TL3] [g ⊢ DR] in
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (fwc (fc_parl FL3 H3 N3)))) (rch (std_par DL DR) id_s*)]
                      | [g ⊢ c_fw DR FR1 FR3 FR2 LR1' LR3] ⇒
                        let [g ⊢ ex_k HL3] = total_key [g ⊢ TL3] in
                        let [g ⊢ HL3]:[g ⊢ key _ KL3[]] = [g ⊢ HL3] in
                        let [g ⊢ FR3]:[g ⊢ fstep_cl _ TR3 _] = [g ⊢ FR3] in
                        let [g ⊢ ex_k HR3] = total_key [g ⊢ TR3] in
                        let [g ⊢ HR3]:[g ⊢ key _ KR3[]] = [g ⊢ HR3] in
                        (case dec_eq_keys [g] [⊢ KL3] [⊢ KR3] of
                           | [g ⊢ c_eq] ⇒
                             let [g ⊢ refl] = functional_lab [g ⊢ LL1] [g ⊢ LL1'] in
                             let [g ⊢ refl] = functional_lab [g ⊢ LR1] [g ⊢ LR1'] in
                             [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (fwc (fc_synr FL3 LL3 HL3 FR3 LR3 HR3)))) (rch (std_par DL DR) id_s*)]
                           | [g ⊢ c_neq I3] ⇒
                             let [g ⊢ NL3] = no_key_in_std [⊢ KL3] [g ⊢ DR] in
                             let [g ⊢ NR3] = lemma_fstep_cl_std [g ⊢ DL] [g ⊢ FL3] [g ⊢ HL3] [g ⊢ I3] in
                             [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (tr_s* (st_s* (sc (fwc (fc_parl FL3 HL3 NL3)))) (st_s* (sc (fwc (fc_parr FR3 HR3 NR3))))) (rch (std_par DL DR) id_s*)])
                      | [g ⊢ c_bw DR FR1 BR3 FR2 _ _] ⇒
                        let [g ⊢ ex_kn _ HL3 NL3] = no_key_of_prlab_in_std [g ⊢ TL3] [g ⊢ DR] in
                        let [g ⊢ FR3]:[g ⊢ fstep_cl _ TR3 _] = loop_lemma_two_cl [g ⊢ BR3] in
                        let [g ⊢ ex_kn _ HR3 NR3] = no_key_of_prlab_in_std [g ⊢ TR3] [g ⊢ DL] in
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (tr_s* (st_s* (sc (bwc (bc_parr BR3 HR3 NR3)))) (st_s* (sc (fwc (fc_parl FL3 HL3 NL3))))) (rch (std_par DL DR) (st_s* (sc (fwc (fc_parr FR3 HR3 NR3)))))])
                 | [g ⊢ c_bw DL FL1 BL3 FL2 LL2' LL3] ⇒ let [g ⊢ FL3]:[g ⊢ fstep_cl _ TL3 _] = loop_lemma_two_cl [g ⊢ BL3] in
                   (case [g ⊢ E2] of
                      | [g ⊢ c_id DR FR1 FR2] ⇒
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ TL3] [g ⊢ DR] in
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (bwc (bc_parl BL3 H3 N3)))) (rch (std_par DL DR) (st_s* (sc (fwc (fc_parl FL3 H3 N3)))))]
                      | [g ⊢ c_fw DR FR1 FR3 FR2 _ _] ⇒
                        let [g ⊢ ex_kn _ HL3 NL3] = no_key_of_prlab_in_std [g ⊢ TL3] [g ⊢ DR] in
                        let [g ⊢ FR3]:[g ⊢ fstep_cl _ TR3 _] = [g ⊢ FR3] in
                        let [g ⊢ ex_kn _ HR3 NR3] = no_key_of_prlab_in_std [g ⊢ TR3] [g ⊢ DL] in
                        [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (tr_s* (st_s* (sc (bwc (bc_parl BL3 HL3 NL3)))) (st_s* (sc (fwc (fc_parr FR3 HR3 NR3))))) (rch (std_par DL DR) (st_s* (sc (fwc (fc_parl FL3 HL3 NL3)))))]
                      | [g ⊢ c_bw DR FR1 BR3 FR2 LR2' LR3] ⇒
                        let [g ⊢ ex_k HL3] = total_key [g ⊢ TL3] in
                        let [g ⊢ HL3]:[g ⊢ key _ KL3[]] = [g ⊢ HL3] in
                        let [g ⊢ FR3]:[g ⊢ fstep_cl _ TR3 _] = loop_lemma_two_cl [g ⊢ BR3] in
                        let [g ⊢ ex_k HR3] = total_key [g ⊢ TR3] in
                        let [g ⊢ HR3]:[g ⊢ key _ KR3[]] = [g ⊢ HR3] in
                        (case dec_eq_keys [g] [⊢ KL3] [⊢ KR3] of
                           | [g ⊢ c_eq] ⇒
                             let [g ⊢ refl] = functional_lab [g ⊢ LL2] [g ⊢ LL2'] in
                             let [g ⊢ refl] = functional_lab [g ⊢ LR2] [g ⊢ LR2'] in
                             [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (bwc (bc_synr BL3 LL3 HL3 BR3 LR3 HR3)))) (rch (std_par DL DR) (st_s* (sc (fwc (fc_synr FL3 LL3 HL3 FR3 LR3 HR3)))))]
                           | [g ⊢ c_neq I3] ⇒
                             let [g ⊢ NL3] = no_key_in_std [⊢ KL3] [g ⊢ DR] in
                             let [g ⊢ NR3] = lemma_fstep_cl_std [g ⊢ DL] [g ⊢ FL3] [g ⊢ HL3] [g ⊢ I3] in
                             [g ⊢ ex_cf (fc_synr FL1 LL1 HL1 FR1 LR1 HR1) (fc_synr FL2 LL2 HL2 FR2 LR2 HR2) (tr_s* (st_s* (sc (bwc (bc_parr BR3 HR3 NR3)))) (st_s* (sc (bwc (bc_parl BL3 HL3 NL3))))) (rch (std_par DL DR) (tr_s* (st_s* (sc (fwc (fc_parl FL3 HL3 NL3)))) (st_s* (sc (fwc (fc_parr FR3 HR3 NR3))))))])))))
;


% We finally prove Theorem 2.1(ii) as a corollary of the previous theorem.

% Type family encoding the existence of two connected transitions involving reachable processes
LF ex_conn_tr: pr_lab_cl → pr_lab_cl → type =
  | ex_c: {S1: step X (p_cl T1) X'} {S2: step Y (p_cl T2) Y'} reachable X → conn_tr S1 S2 → ex_conn_tr T1 T2
;

% Proof of Theorem 2.1(ii):
% If two valid closed proof labels are connected, then there are connected transitions with such labels.
rec conn_rel_two: (g:ctx) [g ⊢ valid_cl T1] → [g ⊢ valid_cl T2] → [g ⊢ conn_cl T1 T2] → [g ⊢ ex_conn_tr T1 T2] =
/ total c (conn_rel_two _ _ _ _ _ c) /
fn v1,v2,c ⇒
let [g ⊢ ex_cf F1 F2 S* (rch D S0*)] = conn_rel_two_fstep v1 v2 c in
let [g ⊢ B2] = loop_lemma_one_cl [g ⊢ F2] in
[g ⊢ ex_c (sc (fwc F1)) (sc (bwc B2)) (rch D S0*) (ct (sc (fwc F1)) (sc (bwc B2)) S*)]
;