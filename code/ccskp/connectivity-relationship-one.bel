%%% Proof of Proposition 4.4 (1) %%%

% If two transitions between reachable processes are connected, then their two proof keyed labels are connected.
rec conn_rel_one: (g:ctx) (S1:[g ⊢ step X T1 X']) (S2:[g ⊢ step Y T2 Y']) [g ⊢ reachable X] → [g ⊢ conn_tr S1 S2] → [g ⊢ conn T1 T2] =
/ total d (conn_rel_one d _ _ _ _ _ _ _ _ _ _) /
fn r,c ⇒ let [g ⊢ ct S1 S2 S*] = c in
let [g ⊢ rch D S0*] = r in
(case [g ⊢ D] of
   | [g ⊢ std_null] ⇒ let [g ⊢ refp] = step*_from_null [g ⊢ S0*] in
     (case [g ⊢ S1] of
        | [g ⊢ sc (fwc F1)] ⇒ impossible [g ⊢ F1]
        | [g ⊢ so (fwo F1)] ⇒ impossible [g ⊢ F1]
        | [g ⊢ sc (bwc B1)] ⇒ impossible [g ⊢ B1]
        | [g ⊢ so (bwo B1)] ⇒ impossible [g ⊢ B1])
   | [g ⊢ std_pref D'] ⇒ let [g ⊢ E1] = step*_from_pref [g ⊢ S0*] in
     let [g ⊢ E2] = step*_from_pref [g ⊢ tr_s* S0* S*] in (case [g ⊢ E1] of
        | [g ⊢ or_pref] ⇒ (case [g ⊢ S1] of
             | [g ⊢ sc (fwc (fc_pref _))] ⇒ (case [g ⊢ S2] of
                  | [g ⊢ sc _] ⇒ [g ⊢ c_cc c_a1]
                  | [g ⊢ so _]:[g ⊢ step _ (p_op (open \_._)) _] ⇒ [g ⊢ c_co \a.c_a1])
             | [g ⊢ so (fwo F1)] ⇒ impossible [g ⊢ F1]
             | [g ⊢ sc (bwc B1)] ⇒ impossible [g ⊢ B1]
             | [g ⊢ so (bwo B1)] ⇒ impossible [g ⊢ B1])
        | [g ⊢ or_kpref] ⇒ (case [g ⊢ E2] of
             | [g ⊢ or_pref] ⇒ (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ impossible [g ⊢ F2]
                  | [g ⊢ so (fwo F2)] ⇒ impossible [g ⊢ F2]
                  | [g ⊢ sc (bwc (bc_pref _))] ⇒ (case [g ⊢ S1] of
                       | [g ⊢ sc _] ⇒ [g ⊢ c_cc c_a2]
                       | [g ⊢ so _]:[g ⊢ step _ (p_op (open \_._)) _] ⇒ [g ⊢ c_oc \a.c_a2])
                  | [g ⊢ so (bwo B2)] ⇒ impossible [g ⊢ B2])
             | [g ⊢ or_kpref] ⇒ let [g ⊢ S*'] = step*_betw_kprefs [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_pref_and_kpref [g ⊢ S0*] in
               (case [g ⊢ S1] of
                  | [g ⊢ sc (fwc (fc_kpref F1' _ _))] ⇒ (case [g ⊢ S2] of
                       | [g ⊢ sc (fwc (fc_pref _))] ⇒ [g ⊢ c_cc c_a2]
                       | [g ⊢ sc (fwc (fc_kpref F2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (fwc F1')) (sc (fwc F2')) S*']
                       | [g ⊢ so (fwo (fo_kpref F2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (fwc F1')) (so (fwo F2')) S*']
                       | [g ⊢ sc (bwc (bc_kpref B2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (fwc F1')) (sc (bwc B2')) S*']
                       | [g ⊢ so (bwo (bo_kpref B2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (fwc F1')) (so (bwo B2')) S*'])
                  | [g ⊢ so (fwo (fo_kpref F1' (\_._) _))] ⇒ (case [g ⊢ S2] of
                       | [g ⊢ sc (fwc (fc_pref _))] ⇒ [g ⊢ c_oc \a.c_a2]
                       | [g ⊢ sc (fwc (fc_kpref F2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (fwo F1')) (sc (fwc F2')) S*']
                       | [g ⊢ so (fwo (fo_kpref F2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (fwo F1')) (so (fwo F2')) S*']
                       | [g ⊢ sc (bwc (bc_kpref B2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (fwo F1')) (sc (bwc B2')) S*']
                       | [g ⊢ so (bwo (bo_kpref B2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (fwo F1')) (so (bwo B2')) S*'])
                  | [g ⊢ sc (bwc (bc_pref _))] ⇒ (case [g ⊢ S2] of
                       | [g ⊢ sc _] ⇒ [g ⊢ c_cc c_a1]
                       | [g ⊢ so _]:[g ⊢ step _ (p_op (open \_._)) _] ⇒ [g ⊢ c_co \a.c_a1])
                  | [g ⊢ sc (bwc (bc_kpref B1' _ _))] ⇒ (case [g ⊢ S2] of
                       | [g ⊢ sc (fwc (fc_pref _))] ⇒ [g ⊢ c_cc c_a2]
                       | [g ⊢ sc (fwc (fc_kpref F2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (bwc B1')) (sc (fwc F2')) S*']
                       | [g ⊢ so (fwo (fo_kpref F2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (bwc B1')) (so (fwo F2')) S*']
                       | [g ⊢ sc (bwc (bc_kpref B2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (bwc B1')) (sc (bwc B2')) S*']
                       | [g ⊢ so (bwo (bo_kpref B2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (sc (bwc B1')) (so (bwo B2')) S*'])
                  | [g ⊢ so (bwo (bo_kpref B1' (\_._) _))] ⇒ (case [g ⊢ S2] of
                       | [g ⊢ sc (fwc (fc_pref _))] ⇒ [g ⊢ c_oc \a.c_a2]
                       | [g ⊢ sc (fwc (fc_kpref F2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (bwo B1')) (sc (fwc F2')) S*']
                       | [g ⊢ so (fwo (fo_kpref F2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (bwo B1')) (so (fwo F2')) S*']
                       | [g ⊢ sc (bwc (bc_kpref B2' _ _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (bwo B1')) (sc (bwc B2')) S*']
                       | [g ⊢ so (bwo (bo_kpref B2' (\_._) _))] ⇒ conn_rel_one [g ⊢ rch D' S0*'] [g ⊢ ct (so (bwo B1')) (so (bwo B2')) S*']))))
   | [g ⊢ std_sum D1 D2] ⇒ let [g ⊢ sm] = step*_from_sum [g ⊢ S0*] in
     let [g ⊢ sm] = step*_from_sum [g ⊢ tr_s* S0* S*] in
     (case [g ⊢ S1] of
        | [g ⊢ sc (fwc F1)] ⇒ (case [g ⊢ F1] of
             | [g ⊢ fc_suml F1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_left [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_left [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (fwc F1')) (sc (fwc F2')) S*'] in [g ⊢ c_cc (c_c1l C)]
                       | [g ⊢ fc_sumr F2' _] ⇒ [g ⊢ c_cc c_c2l])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (fwc F1')) (so (fwo F2')) S*'] in [g ⊢ c_co \a.(c_c1l C)]
                       | [g ⊢ fo_sumr F2' _] ⇒ [g ⊢ c_co \a.c_c2l])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (fwc F1')) (sc (bwc B2')) S*'] in [g ⊢ c_cc (c_c1l C)]
                       | [g ⊢ bc_sumr B2' _] ⇒ [g ⊢ c_cc c_c2l])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bo_suml B2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (fwc F1')) (so (bwo B2')) S*'] in [g ⊢ c_co \a.(c_c1l C)]
                       | [g ⊢ bo_sumr B2' _] ⇒ [g ⊢ c_co \a.c_c2l]))
             | [g ⊢ fc_sumr F1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_right [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_right [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒ [g ⊢ c_cc c_c2r]
                       | [g ⊢ fc_sumr F2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (fwc F1')) (sc (fwc F2')) S*'] in [g ⊢ c_cc (c_c1r C)])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒ [g ⊢ c_co \a.c_c2r]
                       | [g ⊢ fo_sumr F2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (fwc F1')) (so (fwo F2')) S*'] in [g ⊢ c_co \a.(c_c1r C)])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒ [g ⊢ c_cc c_c2r]
                       | [g ⊢ bc_sumr B2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (fwc F1')) (sc (bwc B2')) S*'] in [g ⊢ c_cc (c_c1r C)])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bo_suml B2' _] ⇒ [g ⊢ c_co \a.c_c2r]
                       | [g ⊢ bo_sumr B2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (fwc F1')) (so (bwo B2')) S*'] in [g ⊢ c_co \a.(c_c1r C)])))
        | [g ⊢ so (fwo F1)] ⇒ (case [g ⊢ F1] of
             | [g ⊢ fo_suml F1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_left [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_left [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒
                         let [g ⊢ c_oc \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (fwo F1')) (sc (fwc F2')) S*'] in [g ⊢ c_oc \a.(c_c1l C)]
                       | [g ⊢ fc_sumr F2' _] ⇒ [g ⊢ c_oc \a.c_c2l])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒
                         let [g ⊢ c_oo \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (fwo F1')) (so (fwo F2')) S*'] in [g ⊢ c_oo \a.(c_c1l C)]
                       | [g ⊢ fo_sumr F2' _] ⇒ [g ⊢ c_oo \a.c_c2l])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒
                         let [g ⊢ c_oc \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (fwo F1')) (sc (bwc B2')) S*'] in [g ⊢ c_oc \a.(c_c1l C)]
                       | [g ⊢ bc_sumr B2' _] ⇒ [g ⊢ c_oc \a.c_c2l])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bo_suml B2' _] ⇒
                         let [g ⊢ c_oo \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (fwo F1')) (so (bwo B2')) S*'] in [g ⊢ c_oo \a.(c_c1l C)]
                       | [g ⊢ bo_sumr B2' _] ⇒ [g ⊢ c_oo \a.c_c2l]))
             | [g ⊢ fo_sumr F1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_right [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_right [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒ [g ⊢ c_oc \a.c_c2r]
                       | [g ⊢ fc_sumr F2' _] ⇒
                         let [g ⊢ c_oc \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (so (fwo F1')) (sc (fwc F2')) S*'] in [g ⊢ c_oc \a.(c_c1r C)])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒ [g ⊢ c_oo \a.c_c2r]
                       | [g ⊢ fo_sumr F2' _] ⇒
                         let [g ⊢ c_oo \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (so (fwo F1')) (so (fwo F2')) S*'] in [g ⊢ c_oo \a.(c_c1r C)])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒ [g ⊢ c_oc \a.c_c2r]
                       | [g ⊢ bc_sumr B2' _] ⇒
                         let [g ⊢ c_oc \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (so (fwo F1')) (sc (bwc B2')) S*'] in [g ⊢ c_oc \a.(c_c1r C)])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bo_suml B2' _] ⇒ [g ⊢ c_oo \a.c_c2r]
                       | [g ⊢ bo_sumr B2' _] ⇒
                         let [g ⊢ c_oo \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (so (fwo F1')) (so (bwo B2')) S*'] in [g ⊢ c_oo \a.(c_c1r C)])))
        | [g ⊢ sc (bwc B1)] ⇒ (case [g ⊢ B1] of
             | [g ⊢ bc_suml B1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_left [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_left [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (bwc B1')) (sc (fwc F2')) S*'] in [g ⊢ c_cc (c_c1l C)]
                       | [g ⊢ fc_sumr F2' _] ⇒ [g ⊢ c_cc c_c2l])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (bwc B1')) (so (fwo F2')) S*'] in [g ⊢ c_co \a.(c_c1l C)]
                       | [g ⊢ fo_sumr F2' _] ⇒ [g ⊢ c_co \a.c_c2l])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (bwc B1')) (sc (bwc B2')) S*'] in [g ⊢ c_cc (c_c1l C)]
                       | [g ⊢ bc_sumr B2' _] ⇒ [g ⊢ c_cc c_c2l])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bo_suml B2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (sc (bwc B1')) (so (bwo B2')) S*'] in [g ⊢ c_co \a.(c_c1l C)]
                       | [g ⊢ bo_sumr B2' _] ⇒ [g ⊢ c_co \a.c_c2l]))
             | [g ⊢ bc_sumr B1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_right [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_right [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒ [g ⊢ c_cc c_c2r]
                       | [g ⊢ fc_sumr F2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (bwc B1')) (sc (fwc F2')) S*'] in [g ⊢ c_cc (c_c1r C)])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒ [g ⊢ c_co \a.c_c2r]
                       | [g ⊢ fo_sumr F2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (bwc B1')) (so (fwo F2')) S*'] in [g ⊢ c_co \a.(c_c1r C)])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒ [g ⊢ c_cc c_c2r]
                       | [g ⊢ bc_sumr B2' _] ⇒
                         let [g ⊢ c_cc C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (bwc B1')) (sc (bwc B2')) S*'] in [g ⊢ c_cc (c_c1r C)])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bo_suml B2' _] ⇒ [g ⊢ c_co \a.c_c2r]
                       | [g ⊢ bo_sumr B2' _] ⇒
                         let [g ⊢ c_co \a.C] = conn_rel_one [g ⊢ rch D2 S0*'] [g ⊢ ct (sc (bwc B1')) (so (bwo B2')) S*'] in [g ⊢ c_co \a.(c_c1r C)])))
        | [g ⊢ so (bwo B1)] ⇒ (case [g ⊢ B1] of
             | [g ⊢ bo_suml B1' _] ⇒ let [g ⊢ S*'] = step*_betw_sums_left [g ⊢ S*] in
               let [g ⊢ S0*'] = step*_betw_sums_left [g ⊢ S0*] in
               (case [g ⊢ S2] of
                  | [g ⊢ sc (fwc F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fc_suml F2' _] ⇒
                         let [g ⊢ c_oc \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (bwo B1')) (sc (fwc F2')) S*'] in [g ⊢ c_oc \a.(c_c1l C)]
                       | [g ⊢ fc_sumr F2' _] ⇒ [g ⊢ c_oc \a.c_c2l])
                  | [g ⊢ so (fwo F2)] ⇒ (case [g ⊢ F2] of
                       | [g ⊢ fo_suml F2' _] ⇒
                         let [g ⊢ c_oo \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (bwo B1')) (so (fwo F2')) S*'] in [g ⊢ c_oo \a.(c_c1l C)]
                       | [g ⊢ fo_sumr F2' _] ⇒ [g ⊢ c_oo \a.c_c2l])
                  | [g ⊢ sc (bwc B2)] ⇒ (case [g ⊢ B2] of
                       | [g ⊢ bc_suml B2' _] ⇒
                         let [g ⊢ c_oc \a.C] = conn_rel_one [g ⊢ rch D1 S0*'] [g ⊢ ct (so (bwo B1')) (sc (bwc B2')) S*'] in [g ⊢ c_oc \a.(c_c1l C)]
                       | [g ⊢ bc_sumr B2' _] ⇒ [g ⊢ c_oc \a.c_c2l])
                  | [g ⊢ so (bwo B2)] ⇒ (case [g ⊢ B2] of
