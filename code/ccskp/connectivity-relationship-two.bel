%%% Proof of Theorem 2.1(ii)

% In order to prove Theorem 2.1(ii) (given two connected proof keyed labels T1 and T2, there exist two
% connected transitions whose proof keyed labels are T1 and T2), we first address the base case where the
% label of T1 and T2 is not tau. In this scenario, we prove that the two searched transitions are actually
% two forward transitions, in which at least one of the sources is standard, and the path connecting them
% has at most length one.

% Proof keyed labels whose label is not tau
LF lab_not_tau: pr_lab_cl → type =
  | nt_inp: lab T (inp _) → lab_not_tau T
  | nt_out: lab T (out _) → lab_not_tau T
;

% Type family encoding the existence of forward transitions in which at least one of the sources is standard
% and the path connecting them has at most length one. Additional information on the involved labels is included.
LF max_one_step: pr_lab_cl → pr_lab_cl → type =
  | c_id: std X1 → fstep_cl X1 T1 X1' → fstep_cl X1 T2 X2 → max_one_step T1 T2
  | c_fw: std X1 → fstep_cl X1 T1 X1' → fstep_cl X1 T3 X2' → fstep_cl X2' T2 X2 → lab T1 L1 → lab T3 L1 → max_one_step T1 T2
  | c_bw: std X2' → fstep_cl X1 T1 X1' → bstep_cl X1 T3 X2' → fstep_cl X2' T2 X2 → lab T2 L2 → lab T3 L2 → max_one_step T1 T2
;


% Proof of the base case of Theorem 2.1(ii)
rec conn_rel_two_base: (g:ctx) [g ⊢ valid_cl T1] → [g ⊢ valid_cl T2] → [g ⊢ conn_cl T1 T2] →
  [g ⊢ lab_not_tau T1] → [g ⊢ lab_not_tau T2] → [g ⊢ max_one_step T1 T2] =
/ total c (conn_rel_two_base _ _ _ _ _ c _ _) /
fn v1,v2,c,n1,n2 ⇒ case c of
  | [g ⊢ c_a1] ⇒ let [g ⊢ crl D F]:[g ⊢ realised_cl T2] = pr_lab_is_realised_cl v2 in
    let [g ⊢ ex_k H] = total_key [g ⊢ T2] in
    let [g ⊢ H]:[g ⊢ key _ K[]] = [g ⊢ H] in
    let [g ⊢ ex_n I] = existence_of_different_key [g] [⊢ K] in
    [g ⊢ c_fw (std_pref D) (fc_pref D) (fc_pref D) (fc_kpref F H I) lab_base lab_base]
  | [g ⊢ c_a2] ⇒ let [g ⊢ crl D F]:[g ⊢ realised_cl T1] = pr_lab_is_realised_cl v1 in
    let [g ⊢ ex_k H] = total_key [g ⊢ T1] in
    let [g ⊢ H]:[g ⊢ key _ K[]] = [g ⊢ H] in
    let [g ⊢ ex_n I] = existence_of_different_key [g] [⊢ K] in
    [g ⊢ c_bw (std_pref D) (fc_kpref F H I) (bc_pref D) (fc_pref D) lab_base lab_base]
  | [g ⊢ c_c1l C] ⇒ let [g ⊢ v_suml V1] = v1 in
    let [g ⊢ v_suml V2] = v2 in (case n1 of
       | [g ⊢ nt_inp (lab_suml L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_suml L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F3 std_null) (fc_suml F2 std_null) (lab_suml L1') (lab_suml L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum D std_null) (fc_suml F1 std_null) (bc_suml B3 std_null) (fc_suml F2 std_null) (lab_suml L2') (lab_suml L3')])
            | [g ⊢ nt_out (lab_suml L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F3 std_null) (fc_suml F2 std_null) (lab_suml L1') (lab_suml L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum D std_null) (fc_suml F1 std_null) (bc_suml B3 std_null) (fc_suml F2 std_null) (lab_suml L2') (lab_suml L3')]))
       | [g ⊢ nt_out (lab_suml L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_suml L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F3 std_null) (fc_suml F2 std_null) (lab_suml L1') (lab_suml L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum D std_null) (fc_suml F1 std_null) (bc_suml B3 std_null) (fc_suml F2 std_null) (lab_suml L2') (lab_suml L3')])
            | [g ⊢ nt_out (lab_suml L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum D std_null) (fc_suml F1 std_null) (fc_suml F3 std_null) (fc_suml F2 std_null) (lab_suml L1') (lab_suml L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum D std_null) (fc_suml F1 std_null) (bc_suml B3 std_null) (fc_suml F2 std_null) (lab_suml L2') (lab_suml L3')])))
  | [g ⊢ c_c1r C] ⇒ let [g ⊢ v_sumr V1] = v1 in
    let [g ⊢ v_sumr V2] = v2 in (case n1 of
       | [g ⊢ nt_inp (lab_sumr L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_sumr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F3 std_null) (fc_sumr F2 std_null) (lab_sumr L1') (lab_sumr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum std_null D) (fc_sumr F1 std_null) (bc_sumr B3 std_null) (fc_sumr F2 std_null) (lab_sumr L2') (lab_sumr L3')])
            | [g ⊢ nt_out (lab_sumr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F3 std_null) (fc_sumr F2 std_null) (lab_sumr L1') (lab_sumr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum std_null D) (fc_sumr F1 std_null) (bc_sumr B3 std_null) (fc_sumr F2 std_null) (lab_sumr L2') (lab_sumr L3')]))
       | [g ⊢ nt_out (lab_sumr L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_sumr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F3 std_null) (fc_sumr F2 std_null) (lab_sumr L1') (lab_sumr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum std_null D) (fc_sumr F1 std_null) (bc_sumr B3 std_null) (fc_sumr F2 std_null) (lab_sumr L2') (lab_sumr L3')])
            | [g ⊢ nt_out (lab_sumr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F2 std_null)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ [g ⊢ c_fw (std_sum std_null D) (fc_sumr F1 std_null) (fc_sumr F3 std_null) (fc_sumr F2 std_null) (lab_sumr L1') (lab_sumr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ [g ⊢ c_bw (std_sum std_null D) (fc_sumr F1 std_null) (bc_sumr B3 std_null) (fc_sumr F2 std_null) (lab_sumr L2') (lab_sumr L3')])))
  | [g ⊢ c_c2l] ⇒ let [g ⊢ v_suml V1] = v1 in
    let [g ⊢ v_sumr V2] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    [g ⊢ c_id (std_sum D1 D2) (fc_suml F1 D2) (fc_sumr F2 D1)]
  | [g ⊢ c_c2r] ⇒ let [g ⊢ v_sumr V1] = v1 in
    let [g ⊢ v_suml V2] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    [g ⊢ c_id (std_sum D2 D1) (fc_sumr F1 D2) (fc_suml F2 D1)]
  | [g ⊢ c_p1l C] ⇒ let [g ⊢ v_parl V1]:[g ⊢ valid_cl (pr_parl T1)] = v1 in
    let [g ⊢ v_parl V2]:[g ⊢ valid_cl (pr_parl T2)] = v2 in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ std_null] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ std_null] in
    (case n1 of
       | [g ⊢ nt_inp (lab_parl L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_parl L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L1') (lab_parl L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par D std_null) (fc_parl F1 H1 N1) (bc_parl B3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L2') (lab_parl L3')])
            | [g ⊢ nt_out (lab_parl L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L1') (lab_parl L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par D std_null) (fc_parl F1 H1 N1) (bc_parl B3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L2') (lab_parl L3')]))
       | [g ⊢ nt_out (lab_parl L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_parl L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L1') (lab_parl L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par D std_null) (fc_parl F1 H1 N1) (bc_parl B3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L2') (lab_parl L3')])
            | [g ⊢ nt_out (lab_parl L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par D std_null) (fc_parl F1 H1 N1) (fc_parl F3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L1') (lab_parl L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par D std_null) (fc_parl F1 H1 N1) (bc_parl B3 H3 N3) (fc_parl F2 H2 N2) (lab_parl L2') (lab_parl L3')])))
  | [g ⊢ c_p1r C] ⇒ let [g ⊢ v_parr V1]:[g ⊢ valid_cl (pr_parr T1)] = v1 in
    let [g ⊢ v_parr V2]:[g ⊢ valid_cl (pr_parr T2)] = v2 in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ std_null] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ std_null] in
    (case n1 of
       | [g ⊢ nt_inp (lab_parr L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_parr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L1') (lab_parr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par std_null D) (fc_parr F1 H1 N1) (bc_parr B3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L2') (lab_parr L3')])
            | [g ⊢ nt_out (lab_parr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_inp L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L1') (lab_parr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par std_null D) (fc_parr F1 H1 N1) (bc_parr B3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L2') (lab_parr L3')]))
       | [g ⊢ nt_out (lab_parr L1)] ⇒ (case n2 of
            | [g ⊢ nt_inp (lab_parr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_inp L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L1') (lab_parr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par std_null D) (fc_parr F1 H1 N1) (bc_parr B3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L2') (lab_parr L3')])
            | [g ⊢ nt_out (lab_parr L2)] ⇒ (case conn_rel_two_base [g ⊢ V1] [g ⊢ V2] [g ⊢ C] [g ⊢ nt_out L1] [g ⊢ nt_out L2] of
                 | [g ⊢ c_id D F1 F2] ⇒ [g ⊢ c_id (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F2 H2 N2)]
                 | [g ⊢ c_fw D F1 F3 F2 L1' L3'] ⇒ let [g ⊢ _]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ F3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_fw (std_par std_null D) (fc_parr F1 H1 N1) (fc_parr F3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L1') (lab_parr L3')]
                 | [g ⊢ c_bw D F1 B3 F2 L2' L3'] ⇒ let [g ⊢ _]:[g ⊢ bstep_cl _ T3 _] = [g ⊢ B3] in
                   let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ std_null] in
                   [g ⊢ c_bw (std_par std_null D) (fc_parr F1 H1 N1) (bc_parr B3 H3 N3) (fc_parr F2 H2 N2) (lab_parr L2') (lab_parr L3')])))
  | [g ⊢ c_p2l] ⇒ let [g ⊢ v_parl V1]:[g ⊢ valid_cl (pr_parl T1)] = v1 in
    let [g ⊢ v_parr V2]:[g ⊢ valid_cl (pr_parr T2)] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ D2] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ D1] in
    [g ⊢ c_id (std_par D1 D2) (fc_parl F1 H1 N1) (fc_parr F2 H2 N2)]
  | [g ⊢ c_p2r] ⇒ let [g ⊢ v_parr V1]:[g ⊢ valid_cl (pr_parr T1)] = v1 in
    let [g ⊢ v_parl V2]:[g ⊢ valid_cl (pr_parl T2)] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ D2] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ D1] in
    [g ⊢ c_id (std_par D2 D1) (fc_parr F1 H1 N1) (fc_parl F2 H2 N2)]
  | [g ⊢ c_s1l C] ⇒ (case n2 of
       | [g ⊢ nt_inp L] ⇒ impossible [g ⊢ L]
       | [g ⊢ nt_out L] ⇒ impossible [g ⊢ L])
  | [g ⊢ c_s1r C] ⇒ (case n2 of
       | [g ⊢ nt_inp L] ⇒ impossible [g ⊢ L]
       | [g ⊢ nt_out L] ⇒ impossible [g ⊢ L])
  | [g ⊢ c_s2l C] ⇒ (case n1 of
       | [g ⊢ nt_inp L] ⇒ impossible [g ⊢ L]
       | [g ⊢ nt_out L] ⇒ impossible [g ⊢ L])
  | [g ⊢ c_s2r C] ⇒ (case n1 of
       | [g ⊢ nt_inp L] ⇒ impossible [g ⊢ L]
       | [g ⊢ nt_out L] ⇒ impossible [g ⊢ L])
  | [g ⊢ c_s3 C1 C2] ⇒ (case n1 of
       | [g ⊢ nt_inp L] ⇒ impossible [g ⊢ L]
       | [g ⊢ nt_out L] ⇒ impossible [g ⊢ L])
;


% Type family encoding the existence of two forward closed transitions
% connected by a path and involving reachable processes
LF ex_conn_fstep_cl: pr_lab_cl → pr_lab_cl → type =
  | ex_cf: fstep_cl X1 T1 X1' → fstep_cl X2' T2 X2 → step* X1 X2' → reachable X1 → ex_conn_fstep_cl T1 T2
;

% We prove a stronger version of Theorem 2.1(ii):
% If two valid proof labels are connected, then there are forward transitions
% indexed by such proof keyed labels and connected by a path; moreover,
% the processes involved in such transitions are reachable.
rec conn_rel_two_fstep: (g:ctx) [g ⊢ valid_cl T1] → [g ⊢ valid_cl T2] → [g ⊢ conn_cl T1 T2] → [g ⊢ ex_conn_fstep_cl T1 T2] =
/ total c (conn_rel_two_fstep _ _ _ _ _ c) /
fn v1,v2,c ⇒ case c of
  | [g ⊢ c_a1] ⇒ let [g ⊢ crl D F]:[g ⊢ realised_cl T2] = pr_lab_is_realised_cl v2 in
    let [g ⊢ ex_k H] = total_key [g ⊢ T2] in
    let [g ⊢ H]:[g ⊢ key _ K[]] = [g ⊢ H] in
    let [g ⊢ ex_n I] = existence_of_different_key [g] [⊢ K] in
    [g ⊢ ex_cf (fc_pref D) (fc_kpref F H I) (st_s* (sc (fwc (fc_pref D)))) (rch (std_pref D) id_s*)]
  | [g ⊢ c_a2] ⇒ let [g ⊢ crl D F]:[g ⊢ realised_cl T1] = pr_lab_is_realised_cl v1 in
    let [g ⊢ ex_k H] = total_key [g ⊢ T1] in
    let [g ⊢ H]:[g ⊢ key _ K[]] = [g ⊢ H] in
    let [g ⊢ ex_n I] = existence_of_different_key [g] [⊢ K] in
    [g ⊢ ex_cf (fc_kpref F H I) (fc_pref D) (st_s* (sc (bwc (bc_pref D)))) (rch (std_pref D) (st_s* (sc (fwc (fc_pref D)))))]
  | [g ⊢ c_c1l C] ⇒ let [g ⊢ v_suml V1] = v1 in
    let [g ⊢ v_suml V2] = v2 in
    let [g ⊢ ex_cf F1 F2 S* (rch D S0*)] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ V2] [g ⊢ C] in
    let [g ⊢ S*'] = step*_sumleft_null [g ⊢ S*] in
    let [g ⊢ S0*'] = step*_sumleft_null [g ⊢ S0*] in
    [g ⊢ ex_cf (fc_suml F1 std_null) (fc_suml F2 std_null) S*' (rch (std_sum D std_null) S0*')]
  | [g ⊢ c_c1r C] ⇒ let [g ⊢ v_sumr V1] = v1 in
    let [g ⊢ v_sumr V2] = v2 in
    let [g ⊢ ex_cf F1 F2 S* (rch D S0*)] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ V2] [g ⊢ C] in
    let [g ⊢ S*'] = step*_sumright_null [g ⊢ S*] in
    let [g ⊢ S0*'] = step*_sumright_null [g ⊢ S0*] in
    [g ⊢ ex_cf (fc_sumr F1 std_null) (fc_sumr F2 std_null) S*' (rch (std_sum std_null D) S0*')]
  | [g ⊢ c_c2l] ⇒ let [g ⊢ v_suml V1] = v1 in
    let [g ⊢ v_sumr V2] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    [g ⊢ ex_cf (fc_suml F1 D2) (fc_sumr F2 D1) id_s* (rch (std_sum D1 D2) id_s*)]
  | [g ⊢ c_c2r] ⇒ let [g ⊢ v_sumr V1] = v1 in
    let [g ⊢ v_suml V2] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    [g ⊢ ex_cf (fc_sumr F1 D2) (fc_suml F2 D1) id_s* (rch (std_sum D2 D1) id_s*)]
  | [g ⊢ c_p1l C] ⇒ let [g ⊢ v_parl V1] = v1 in
    let [g ⊢ v_parl V2] = v2 in
    let [g ⊢ ex_cf F1 F2 S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl T1 T2] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ V2] [g ⊢ C] in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ std_null] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ std_null] in
    let [g ⊢ S*'] = step*_parleft_std [g ⊢ S*] [g ⊢ std_null] in
    let [g ⊢ S0*'] = step*_parleft_std [g ⊢ S0*] [g ⊢ std_null] in
    [g ⊢ ex_cf (fc_parl F1 H1 N1) (fc_parl F2 H2 N2) S*' (rch (std_par D std_null) S0*')]
  | [g ⊢ c_p1r C] ⇒ let [g ⊢ v_parr V1] = v1 in
    let [g ⊢ v_parr V2] = v2 in
    let [g ⊢ ex_cf F1 F2 S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl T1 T2] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ V2] [g ⊢ C] in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ std_null] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ std_null] in
    let [g ⊢ S*'] = step*_parright_std [g ⊢ S*] [g ⊢ std_null] in
    let [g ⊢ S0*'] = step*_parright_std [g ⊢ S0*] [g ⊢ std_null] in
    [g ⊢ ex_cf (fc_parr F1 H1 N1) (fc_parr F2 H2 N2) S*' (rch (std_par std_null D) S0*')]
  | [g ⊢ c_p2l] ⇒ let [g ⊢ v_parl V1]:[g ⊢ valid_cl (pr_parl T1)] = v1 in
    let [g ⊢ v_parr V2]:[g ⊢ valid_cl (pr_parr T2)] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ D2] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ D1] in
    [g ⊢ ex_cf (fc_parl F1 H1 N1) (fc_parr F2 H2 N2) id_s* (rch (std_par D1 D2) id_s*)]
  | [g ⊢ c_p2r] ⇒ let [g ⊢ v_parr V1]:[g ⊢ valid_cl (pr_parr T1)] = v1 in
    let [g ⊢ v_parl V2]:[g ⊢ valid_cl (pr_parl T2)] = v2 in
    let [g ⊢ crl D1 F1] = pr_lab_is_realised_cl [g ⊢ V1] in
    let [g ⊢ crl D2 F2] = pr_lab_is_realised_cl [g ⊢ V2] in
    let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ D2] in
    let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ D1] in
    [g ⊢ ex_cf (fc_parr F1 H1 N1) (fc_parl F2 H2 N2) id_s* (rch (std_par D2 D1) id_s*)]
  | [g ⊢ c_s1l C] ⇒ let [g ⊢ v_parl V1] = v1 in (case v2 of
       | [g ⊢ v_synl VL VR LL HL LR HR] ⇒ let [g ⊢ crl DR FR] = pr_lab_is_realised_cl [g ⊢ VR] in
         let [g ⊢ ex_cf F1 FL S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl T1 _] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ VL] [g ⊢ C] in
         let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ DR] in
         let [g ⊢ S0*'] = step*_parleft_std [g ⊢ S0*] [g ⊢ DR] in
         let [g ⊢ S*'] = step*_parleft_std [g ⊢ S*] [g ⊢ DR] in
         [g ⊢ ex_cf (fc_parl F1 H1 N1) (fc_synl FL LL HL FR LR HR) S*' (rch (std_par D DR) S0*')]
       | [g ⊢ v_synr VL VR LL HL LR HR] ⇒ let [g ⊢ crl DR FR] = pr_lab_is_realised_cl [g ⊢ VR] in
         let [g ⊢ ex_cf F1 FL S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl T1 _] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ VL] [g ⊢ C] in
         let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ DR] in
         let [g ⊢ S0*'] = step*_parleft_std [g ⊢ S0*] [g ⊢ DR] in
         let [g ⊢ S*'] = step*_parleft_std [g ⊢ S*] [g ⊢ DR] in
         [g ⊢ ex_cf (fc_parl F1 H1 N1) (fc_synr FL LL HL FR LR HR) S*' (rch (std_par D DR) S0*')])
  | [g ⊢ c_s1r C] ⇒ let [g ⊢ v_parr V1] = v1 in (case v2 of
       | [g ⊢ v_synl VL VR LL HL LR HR] ⇒ let [g ⊢ crl DL FL] = pr_lab_is_realised_cl [g ⊢ VL] in
         let [g ⊢ ex_cf F1 FR S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl T1 _] = conn_rel_two_fstep [g ⊢ V1] [g ⊢ VR] [g ⊢ C] in
         let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ DL] in
         let [g ⊢ S0*'] = step*_parright_std [g ⊢ S0*] [g ⊢ DL] in
         let [g ⊢ S*'] = step*_parright_std [g ⊢ S*] [g ⊢ DL] in
         [g ⊢ ex_cf (fc_parr F1 H1 N1) (fc_synl FL LL HL FR LR HR) S*' (rch (std_par DL D) S0*')]
       | [g ⊢ v_synr VL VR LL HL LR HR] ⇒ let [g ⊢ crl DL FL] = pr_lab_is_realised_cl [g ⊢ VL] in
