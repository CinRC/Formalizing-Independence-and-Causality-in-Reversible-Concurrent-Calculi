         let [g ⊢ ex_kn _ H1 N1] = no_key_of_prlab_in_std [g ⊢ T1] [g ⊢ DL] in
         let [g ⊢ S0*'] = step*_parright_std [g ⊢ S0*] [g ⊢ DL] in
         let [g ⊢ S*'] = step*_parright_std [g ⊢ S*] [g ⊢ DL] in
         [g ⊢ ex_cf (fc_parr F1 H1 N1) (fc_synr FL LL HL FR LR HR) S*' (rch (std_par DL D) S0*')])
  | [g ⊢ c_s2l C] ⇒ let [g ⊢ v_parl V2] = v2 in (case v1 of
       | [g ⊢ v_synl VL VR LL HL LR HR] ⇒ let [g ⊢ crl DR FR] = pr_lab_is_realised_cl [g ⊢ VR] in
         let [g ⊢ ex_cf FL F2 S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl _ T2] = conn_rel_two_fstep [g ⊢ VL] [g ⊢ V2] [g ⊢ C] in
         let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ DR] in
         let [g ⊢ S0*'] = step*_parleft_std [g ⊢ S0*] [g ⊢ DR] in
         let [g ⊢ S*'] = step*_parleft_std [g ⊢ S*] [g ⊢ DR] in
         [g ⊢ ex_cf (fc_synl FL LL HL FR LR HR) (fc_parl F2 H2 N2) S*' (rch (std_par D DR) S0*')]
       | [g ⊢ v_synr VL VR LL HL LR HR] ⇒ let [g ⊢ crl DR FR] = pr_lab_is_realised_cl [g ⊢ VR] in
         let [g ⊢ ex_cf FL F2 S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl _ T2] = conn_rel_two_fstep [g ⊢ VL] [g ⊢ V2] [g ⊢ C] in
         let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ DR] in
         let [g ⊢ S0*'] = step*_parleft_std [g ⊢ S0*] [g ⊢ DR] in
         let [g ⊢ S*'] = step*_parleft_std [g ⊢ S*] [g ⊢ DR] in
         [g ⊢ ex_cf (fc_synr FL LL HL FR LR HR) (fc_parl F2 H2 N2) S*' (rch (std_par D DR) S0*')])
  | [g ⊢ c_s2r C] ⇒ let [g ⊢ v_parr V2] = v2 in (case v1 of
       | [g ⊢ v_synl VL VR LL HL LR HR] ⇒ let [g ⊢ crl DL FL] = pr_lab_is_realised_cl [g ⊢ VL] in
         let [g ⊢ ex_cf FR F2 S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl _ T2] = conn_rel_two_fstep [g ⊢ VR] [g ⊢ V2] [g ⊢ C] in
         let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ DL] in
         let [g ⊢ S0*'] = step*_parright_std [g ⊢ S0*] [g ⊢ DL] in
         let [g ⊢ S*'] = step*_parright_std [g ⊢ S*] [g ⊢ DL] in
         [g ⊢ ex_cf (fc_synl FL LL HL FR LR HR) (fc_parr F2 H2 N2) S*' (rch (std_par DL D) S0*')]
       | [g ⊢ v_synr VL VR LL HL LR HR] ⇒ let [g ⊢ crl DL FL] = pr_lab_is_realised_cl [g ⊢ VL] in
         let [g ⊢ ex_cf FR F2 S* (rch D S0*)]:[g ⊢ ex_conn_fstep_cl _ T2] = conn_rel_two_fstep [g ⊢ VR] [g ⊢ V2] [g ⊢ C] in
         let [g ⊢ ex_kn _ H2 N2] = no_key_of_prlab_in_std [g ⊢ T2] [g ⊢ DL] in
         let [g ⊢ S0*'] = step*_parright_std [g ⊢ S0*] [g ⊢ DL] in
         let [g ⊢ S*'] = step*_parright_std [g ⊢ S*] [g ⊢ DL] in
         [g ⊢ ex_cf (fc_synr FL LL HL FR LR HR) (fc_parr F2 H2 N2) S*' (rch (std_par DL D) S0*')])
  | [g ⊢ c_s3 C1 C2] ⇒ (case v1 of
       | [g ⊢ v_synl VL1 VR1 LL1 HL1 LR1 HR1] ⇒ (case v2 of
            | [g ⊢ v_synl VL2 VR2 LL2 HL2 LR2 HR2] ⇒ 
              let [g ⊢ E1] = conn_rel_two_base [g ⊢ VL1] [g ⊢ VL2] [g ⊢ C1] [g ⊢ nt_inp LL1] [g ⊢ nt_inp LL2] in
              let [g ⊢ E2] = conn_rel_two_base [g ⊢ VR1] [g ⊢ VR2] [g ⊢ C2] [g ⊢ nt_out LR1] [g ⊢ nt_out LR2] in
              (case [g ⊢ E1] of
                 | [g ⊢ c_id DL FL1 FL2] ⇒ (case [g ⊢ E2] of
                      | [g ⊢ c_id DR FR1 FR2] ⇒
                        [g ⊢ ex_cf (fc_synl FL1 LL1 HL1 FR1 LR1 HR1) (fc_synl FL2 LL2 HL2 FR2 LR2 HR2) id_s* (rch (std_par DL DR) id_s*)]
                      | [g ⊢ c_fw DR FR1 FR3 FR2 _ _] ⇒ let [g ⊢ FR3]:[g ⊢ fstep_cl _ T3 _] = [g ⊢ FR3] in
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ DL] in
                        [g ⊢ ex_cf (fc_synl FL1 LL1 HL1 FR1 LR1 HR1) (fc_synl FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (fwc (fc_parr FR3 H3 N3)))) (rch (std_par DL DR) id_s*)]
                      | [g ⊢ c_bw DR FR1 BR3 FR2 _ _] ⇒ let [g ⊢ FR3]:[g ⊢ fstep_cl _ T3 _] = loop_lemma_two_cl [g ⊢ BR3] in
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ T3] [g ⊢ DL] in
                        [g ⊢ ex_cf (fc_synl FL1 LL1 HL1 FR1 LR1 HR1) (fc_synl FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (bwc (bc_parr BR3 H3 N3)))) (rch (std_par DL DR) (st_s* (sc (fwc (fc_parr FR3 H3 N3)))))])
                 | [g ⊢ c_fw DL FL1 FL3 FL2 LL1' LL3] ⇒ let [g ⊢ FL3]:[g ⊢ fstep_cl _ TL3 _] = [g ⊢ FL3] in
                   (case [g ⊢ E2] of
                      | [g ⊢ c_id DR FR1 FR2] ⇒
                        let [g ⊢ ex_kn _ H3 N3] = no_key_of_prlab_in_std [g ⊢ TL3] [g ⊢ DR] in
                        [g ⊢ ex_cf (fc_synl FL1 LL1 HL1 FR1 LR1 HR1) (fc_synl FL2 LL2 HL2 FR2 LR2 HR2) (st_s* (sc (fwc (fc_parl FL3 H3 N3)))) (rch (std_par DL DR) id_s*)]
                      | [g ⊢ c_fw DR FR1 FR3 FR2 LR1' LR3] ⇒
